        <com.zhijia.ui.view.MyScrollView
            android:id="@+id/old_house_details_scrollView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/old_house_details_tier"
            android:layout_below="@+id/old_house_head"
            android:background="@color/common_bg"
            android:scrollbars="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <RelativeLayout
                    android:id="@+id/index_splash_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <!-- 轮播页 -->

                    <android.support.v4.view.ViewPager
                        android:id="@+id/old_house_details_house_image"
                        android:layout_width="match_parent"
                        android:layout_height="450px"
                        android:focusable="true" />

                    <TextView
                        android:id="@+id/old_house_details_house_image_text_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignRight="@+id/old_house_details_house_image"
                        android:layout_marginBottom="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/black"
                        android:padding="5dp"
                        android:text="共有21"
                        android:textColor="@color/white" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:gravity="center"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:id="@+id/old_house_details_house_dotsId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="3dip"
                            android:gravity="center"
                            android:orientation="horizontal" />
                    </LinearLayout>
                </RelativeLayout>

                <ImageView
                    android:id="@+id/old_house_details_house_split_line_one"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginTop="5dp"
                    android:background="@color/split_line" />
                <!-- 房子的介绍布局 -->

                <RelativeLayout
                    android:id="@+id/old_house_details_house_introduce"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp" >

                    <TextView
                        android:id="@+id/old_house_details_house_introduce_content"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:text="红星花园无双税首付17万精装修双学区片内急售"
                        android:textColor="@color/black"
                        android:textSize="14dp" />

                    <ImageView
                        android:id="@+id/old_house_details_time_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/old_house_details_house_introduce_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/time" />

                    <TextView
                        android:id="@+id/old_house_details_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/old_house_details_house_introduce_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_toRightOf="@+id/old_house_details_time_image"
                        android:text="今天18:44更新"
                        android:textColor="@color/dark_bg" />

                    <TextView
                        android:id="@+id/old_house_details_impression"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/old_house_details_house_introduce_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_toRightOf="@+id/old_house_details_time"
                        android:background="@drawable/mid_button"
                        android:padding="2dp"
                        android:text="@string/invest_sift"
                        android:textColor="@color/black"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/old_house_details_impression_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/old_house_details_house_introduce_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_toRightOf="@+id/old_house_details_impression"
                        android:background="@drawable/mid_button"
                        android:padding="2dp"
                        android:text="@string/elevator_house"
                        android:textColor="@color/black"
                        android:textSize="12dp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/old_house_details_house_split_line_two"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginTop="5dp"
                    android:background="@color/split_line" />
