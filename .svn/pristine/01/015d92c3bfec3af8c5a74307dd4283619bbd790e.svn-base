                        <TextView
                            android:id="@+id/old_house_details_introduce_price_area"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/proportion"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_area_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_toRightOf="@+id/old_house_details_introduce_price_area"
                            android:text="75.6m²"
                            android:textColor="@color/dark_bg" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_orientation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_area"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/orientation"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_orientation_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_area_value"
                            android:layout_marginLeft="10dp"
