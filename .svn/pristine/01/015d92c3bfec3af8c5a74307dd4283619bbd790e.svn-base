                            android:layout_toRightOf="@+id/old_house_details_introduce_price_house_type"
                            android:text="住宅"
                            android:textColor="@color/dark_bg" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_equity"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_house_type"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/equity"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_equity_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_house_type_value"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_toRightOf="@+id/old_house_details_introduce_price_equity"
                            android:text="70年"
                            android:textColor="@color/dark_bg" />
                    </RelativeLayout>
                    <!-- 右边的布局 -->

                    <RelativeLayout
                        android:id="@+id/old_house_details_introduce_right"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/old_house_details_introduce_price_money"
                        android:layout_toRightOf="@+id/old_house_details_introduce_left" >

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/room_type"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_type_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_toRightOf="@+id/old_house_details_introduce_price_type"
                            android:text="2室2厅1卫"
                            android:textColor="@color/dark_bg" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_floors"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_type"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/floor"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_floors_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_type_value"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_toRightOf="@+id/old_house_details_introduce_price_floors"
                            android:text="第2层/第7层"
                            android:textColor="@color/dark_bg" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_decorate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_floors"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/decorate"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_decorate_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_floors_value"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_toRightOf="@+id/old_house_details_introduce_price_decorate"
                            android:text="精装"
                            android:textColor="@color/dark_bg" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_house_age"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_decorate"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="5dp"
                            android:text="@string/house_age"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/old_house_details_introduce_price_house_age_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/old_house_details_introduce_price_decorate_value"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_toRightOf="@+id/old_house_details_introduce_price_house_age"
                            android:text="2000年"
                            android:textColor="@color/dark_bg" />
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:id="@+id/old_house_details_house_description"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/bg_blue"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    android:text="@string/house_description"
                    android:textColor="@color/white" />
                <!-- 房子的描述 -->

                <LinearLayout
                    android:id="@+id/old_house_details_description_linear"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <com.zhijia.ui.view.ZjGridView
                        android:id="@+id/old_house_details_description_grid"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/common_bg"
                        android:clickable="false"
                        android:gravity="center_vertical"
                        android:horizontalSpacing="5dp"
                        android:numColumns="3"
                        android:padding="5dp"
                        android:verticalSpacing="10dp" />

                    <ImageView
                        android:id="@+id/old_house_details_house_split_line_three"
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_below="@+id/old_house_details_description_grid"
                        android:layout_marginTop="5dp"
                        android:background="@color/split_line" />

                    <ListView
                        android:id="@+id/old_house_details_description_list_view"
                        android:layout_width="match_parent"
